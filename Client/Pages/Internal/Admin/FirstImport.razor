@page "/admin/first-import"
@inherits FirstImportBase

<h3>Process after imported</h3>

@if (IsValidating)
{
    <LoadingProcess/>
}
else
{
    <div class="mt-3 mt-xl-5">
        <DxButton Text="Start Process"
                  SizeMode="SizeMode.Large"
                  Visible="@ButtonVisible"
                  Click="@StartProcess"/>
    </div>
}

<div class="pt-5"></div>